#!/bin/bash

CALLS="10000000"
FLAGS="1111111"

if [ $# -eq 2 ]
then
FLAGS=$1
CALLS=$2
fi

if [ $# -eq 1 ]
then
FLAGS=$1
fi


./bin/Integrate_pp_ttX -I $FLAGS -N $CALLS -E 13 -D --add_higgs 500,30.53,0.8631,0.9881,-0.6420,0.4842 --add_higgs 800,148.61,-1.1572,0.9881,0.3480,0.4842  -v 1 -L -F -r 0.5 -K -Q
